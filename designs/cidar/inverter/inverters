/*
 * Properties
 */
Property ID(txt);
Property sequence(txt);
Property repress(txt);
Property repressedby(txt);
Property induce(txt);
Property inducedby(txt);
Property pigeon(txt);
Property strength(num); 

/*
 * CONSTITUTIVE PROMOTERS
 */
PartType ConstPr (ID, sequence, pigeon);
ConstPr pConst("J23100", "TTGACGGCTAGCTCAGTCCTAGGTACAGTGCTAGC", "p ConstPr 14");

/*
 * REPRESSIBLE PROMOTERS
 */
PartType ReprPr (ID, sequence, repressedby, pigeon);
ReprPr pAraBAD1("I13453", "ACATTGATTATTTGCACGGCGTCACACTTTGCTATGCCATAGCATTTTTATCCATAAGATTAGCGGATCCTACCTGACGCTTTTTATCGCAACTCTCTACTGTTTCTCCATACCGTTTTTTTGGGCTAGC", "AraC", "c AraC 10");
ReprPr pAraBAD2("I13453", "ACATTGATTATTTGCACGGCGTCACACTTTGCTATGCCATAGCATTTTTATCCATAAGATTAGCGGATCCTACCTGACGCTTTTTATCGCAACTCTCTACTGTTTCTCCATACCGTTTTTTTGGGCTAGC", "AraC", "c AraC 10");
ReprPr pTetR("R0040", "TCCCTATCAGTGATAGAGATTGACATCCCTATCAGTGATAGAGATACTGAGCAC", "TetR", "p pTetR 2");
ReprPr pcI("R0051", "TAACACCGTGCGTGTTGACTATTTTACCTCTGGCGGTGATAATGGTTGC", "cI", "p pcI 8");
ReprPr pLacI("R0010", "CAATACGCAAACCGCCTCTCCCCGCGCGTTGGCCGATTCATTAATGCAGCTGGCACGACAGGTTTCCCGACTGGAAAGCGGGCAGTGAGCGCAACGCAATTAATGTGAGTTAGCTCACTCATTAGGCACCCCAGGCTTTACACTTTATGCTTCCGGCTCGTATGTTGTGTGGAATTGTGAGCGGATAACAATTTCACACA", "LacI", "p LacI 13");
ReprPr pLuxRr("R0063", "ACCTGTACGATCCTACAGGTGCTTATGTTAAGTAATTGTATTCCCAGCGATACAATAGTGTGACAAAAATCCAATTTATTAGAATCAAATGTCAATCCATTACCGTTTTAATGATATATAACACGCAAAACTTGCGACAAACAATAGGTAA", "rLuxR", "p LuxRr 7");

/*
 * INDUCIBLE PROMOTERS
 */
PartType IndPr (ID, sequence, inducedby, pigeon);
IndPr pLuxRi("R0062", "acctgtaggatcgtacaggtttacgcaagaaaatggtttgttatagtcgaataaa", "iLuxR", "p pLuxRi 7");

/*
 * RBSs
 */
PartType RBS (ID, sequence, strength, pigeon);
RBS RBS30m("B0030m", "AGAGATTAAAGAGGAGAAATACTA", 1, "r r 1 nl");
RBS RBS31m("B0031m", "AGAGTCACACAGGAAACCTACTA", 2, "r r 3 nl");
RBS RBS32m("B0032m", "AGAGTCACACAGGAAAGTACTA", 3, "r r 5 nl");
RBS RBS33m("B0033m", " AGAGTCACACAGGACTACTA", 5, "r r 9 nl");
RBS RBS34m1("B0034m1", "AGAGAAAGAGGAGAAATACTA", 2, "r r 11 nl");


/*
 * REPRESSORS
 */
PartType Repressor (ID, sequence, repress, pigeon);
Repressor AraC("I13453", "", "pAraBAD", "c AraC 10");
Repressor TetR("C0040", "", "pTetR", "c TetR 2");
Repressor cI("C0051", "", "pcI", "c cI 8");
Repressor LacI("C0012m", "", "pLacI", "c LacI 13");
Repressor rLuxR("C0062", "", "pLuxRr", "c LuxR 7");

/*
 * INDUCERS
 */
PartType Inducer (ID, sequence, induce, pigeon);
Inducer iLuxR("C0062", "", "pLuxRi", "c LuxR 7");

/*
 * REPORTERS
 */
PartType Reporter (ID, sequence, pigeon);
Reporter RFP ("E1010m", "", "c RFP 6");
Reporter GFP ("E0040", "", "c GFP 4");

/*
 * TERMINATOR
 */
PartType Terminator (ID, sequence, pigeon);
Terminator term("B0015", "AGGTCCAGGCATCAAATAAAACGAAAGGCTCAGTCGAAAGACTGGGCCTTTCGTTTTATCTGTTGTTTGTCGGTGAACGCTCTCTACTAGAGTCACACTGGCTCACCTTCGGGTGGGCCTTTCTGCGTTTATAGCTT", "t t 14 nl");

/*
 * SMALL MOLECULES
 */
PartType IndSmallMolecule (induce, pigeon);
IndSmallMolecule AHL ("LuxR", "");
PartType RepSmallMolecule (repress, pigeon);
RepSmallMolecule LAra ("AraC", "");
RepSmallMolecule aTc ("TetR", "");


Device Inverter(
    [pAraBAD1|pAraBAD2|pConst|pTetR], [RBS30m|RBS31m|RBS32m|RBS33m|RBS34m1], [AraC|TetR|RFP|GFP], term, 
    [pAraBAD1|pAraBAD2|pConst|pTetR], [RBS30m|RBS31m|RBS32m|RBS33m|RBS34m1], [AraC|TetR|RFP|GFP], term, 
    [pAraBAD1|pAraBAD2|pConst|pTetR], [RBS30m|RBS31m|RBS32m|RBS33m|RBS34m1], [AraC|TetR|RFP|GFP], term, 
    [pAraBAD1|pAraBAD2|pConst|pTetR], [RBS30m|RBS31m|RBS32m|RBS33m|RBS34m1], [AraC|TetR|RFP|GFP], term);

/*
 *   RULES
 */
Rule r(
    on Inverter:
    RFP with GFP and
    RFP exactly 1 and
    GFP exactly 1 and
    
    not RBS30m nextto GFP or RBS30m nextto TetR and
    not GFP nextto RBS30m or TetR nextto RBS30m and
    not RBS30m nextto TetR or RBS30m nextto GFP and  
    not TetR nextto RBS30m or GFP nextto RBS30m and
    
    not RBS31m nextto GFP or RBS31m nextto TetR and
    not GFP nextto RBS31m or TetR nextto RBS31m and
    not RBS31m nextto TetR or RBS31m nextto GFP and  
    not TetR nextto RBS31m or GFP nextto RBS31m and

    not RBS32m nextto GFP or RBS32m nextto TetR and
    not GFP nextto RBS32m or TetR nextto RBS32m and
    not RBS32m nextto TetR or RBS32m nextto GFP and  
    not TetR nextto RBS32m or GFP nextto RBS32m and
    
    not RBS33m nextto GFP or RBS33m nextto TetR and
    not GFP nextto RBS33m or TetR nextto RBS33m and
    not RBS33m nextto TetR or RBS33m nextto GFP and  
    not TetR nextto RBS33m or GFP nextto RBS33m and 

    not RBS34m1 nextto GFP or RBS34m1 nextto TetR and
    not GFP nextto RBS34m1 or TetR nextto RBS34m1 and
    not RBS34m1 nextto TetR or RBS34m1 nextto GFP and  
    not TetR nextto RBS34m1 or GFP nextto RBS34m1 and
    
    AraC exactly 1 and
    pConst exactly 1 and
    pConst drives AraC and

    TetR exactly 1 and
    pAraBAD1 exactly 1 and
    pAraBAD1 drives TetR and
    pAraBAD2 exactly 1 and
    pAraBAD2 drives GFP and

    TetR represses pTetR and
    AraC represses pAraBAD1 and
    AraC represses pAraBAD2 and

    all_forward);	

product(Inverter);


