 */
PartType RBS (ID, sequence, strength);
RBS RBS30m("B0030m", "AGAGATTAAAGAGGAGAAATACTA", 1);
RBS RBS31m("B0031m", "AGAGTCACACAGGAAACCTACTA", 2);
RBS RBS32m("B0032m", "AGAGTCACACAGGAAAGTACTA", 3);
RBS RBS33m("B0033m", " AGAGTCACACAGGACTACTA", 5);
RBS RBS34m1("B0034m1", "AGAGAAAGAGGAGAAATACTA", 2);


/*
 * REPRESSORS
 */
PartType Repressor (ID, sequence, repress);
Repressor AraC("I13453", "", "pAraBAD");
Repressor TetR("C0040", "", "pTetR");
Repressor cI("C0051", "", "pcI");
Repressor LacI("C0012m", "", "pLacI");
Repressor rLuxR("C0062", "", "pLuxRr");

/*
 * INDUCERS
 */
PartType Inducer (ID, sequence, induce);
Inducer iLuxR("C0062", "", "pLuxRi");

/*
 * REPORTERS
 */
PartType Reporter (ID, sequence);
Reporter RFP ("E1010m", "");
Reporter GFP ("E0040", "");

/*
 * TERMINATOR
 */
PartType Terminator (ID, sequence);
Terminator term("B0015", "AGGTCCAGGCATCAAATAAAACGAAAGGCTCAGTCGAAAGACTGGGCCTTTCGTTTTATCTGTTGTTTGTCGGTGAACGCTCTCTACTAGAGTCACACTGGCTCACCTTCGGGTGGGCCTTTCTGCGTTTATAGCTT");

/*
 * SMALL MOLECULES
 */
PartType IndSmallMolecule (induce);
IndSmallMolecule AHL ("LuxR");
PartType RepSmallMolecule (repress);
RepSmallMolecule LAra ("AraC");
RepSmallMolecule aTc ("TetR");


Device Inverter(
    [pAraBAD1|pAraBAD2|pConst|pTetR], [RBS30m|RBS31m|RBS32m|RBS33m|RBS34m1], [AraC|TetR|RFP|GFP], term, 
    [pAraBAD1|pAraBAD2|pConst|pTetR], [RBS30m|RBS31m|RBS32m|RBS33m|RBS34m1], [AraC|TetR|RFP|GFP], term, 
    [pAraBAD1|pAraBAD2|pConst|pTetR], [RBS30m|RBS31m|RBS32m|RBS33m|RBS34m1], [AraC|TetR|RFP|GFP], term, 
    [pAraBAD1|pAraBAD2|pConst|pTetR], [RBS30m|RBS31m|RBS32m|RBS33m|RBS34m1], [AraC|TetR|RFP|GFP], term);

/*
 *   RULES
 */
Rule r(
    on Inverter:
    RFP with GFP and
    RFP exactly 1 and
    GFP exactly 1 and
    
    not RBS30m nextto GFP or RBS30m nextto TetR and
    not GFP nextto RBS30m or TetR nextto RBS30m and
    not RBS30m nextto TetR or RBS30m nextto GFP and  
    not TetR nextto RBS30m or GFP nextto RBS30m and
    
    not RBS31m nextto GFP or RBS31m nextto TetR and
    not GFP nextto RBS31m or TetR nextto RBS31m and
    not RBS31m nextto TetR or RBS31m nextto GFP and  
    not TetR nextto RBS31m or GFP nextto RBS31m and

    not RBS32m nextto GFP or RBS32m nextto TetR and
    not GFP nextto RBS32m or TetR nextto RBS32m and
    not RBS32m nextto TetR or RBS32m nextto GFP and  
    not TetR nextto RBS32m or GFP nextto RBS32m and
    
    not RBS33m nextto GFP or RBS33m nextto TetR and
    not GFP nextto RBS33m or TetR nextto RBS33m and
    not RBS33m nextto TetR or RBS33m nextto GFP and  
    not TetR nextto RBS33m or GFP nextto RBS33m and 

    not RBS34m1 nextto GFP or RBS34m1 nextto TetR and
    not GFP nextto RBS34m1 or TetR nextto RBS34m1 and
    not RBS34m1 nextto TetR or RBS34m1 nextto GFP and  
    not TetR nextto RBS34m1 or GFP nextto RBS34m1 and
    
    AraC exactly 1 and
    pConst exactly 1 and
    pConst drives AraC and

    TetR exactly 1 and
    pAraBAD1 exactly 1 and
    pAraBAD1 drives TetR and
    pAraBAD2 exactly 1 and
    pAraBAD2 drives GFP and

    TetR represses pTetR and
    AraC represses pAraBAD1 and
    AraC represses pAraBAD2 and

    all_forward);	

product(Inverter);


