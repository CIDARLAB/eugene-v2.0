//transcriptional units
//TU1: promoter: const, gene: repressor1
Device driverTU(Const_promoter, RBS, repressor, terminator);

//TU2: promoter: repressible1, gene: repressor2
Device repTU(Repr_promoter, RBS, repressor, terminator);

//TU3: promoter: repressible1, gene:signal1
Device gfpRepCountTU(Repr_promoter, RBS, signal, terminator);

//TU4: promoter: repressible2, gene: signal2
Device rfpOutputTU(Repr_promoter, RBS, signal, terminator);

// INVERTER device (i.e. a composition of the four TUs)
Device inverter(driverTU, repTU, gfpRepCountTU, rfpOutputTU);

//Rules

Rule matchDriverReprw_promotergfp(gfpRepCountTU[0].repressedby == driverTU[2].name);
Rule matchDriverReprw_promoterrep(repTU[0].repressedby == driverTU[2].name); 
Rule matchrepTURepw_promoterrfp(rfpOutputTU[0].repressedby == repTU[2].name);

/***
