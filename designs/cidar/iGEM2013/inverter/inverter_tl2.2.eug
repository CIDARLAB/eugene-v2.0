//fusion sites rules
Rule d1d2(repTU[0].LFS == driverTU[3].RFS);
Rule d2d3(gfpRepCountTU[0].LFS == repTU[3].RFS);
Rule d3d4(rfpOutputTU[0].LFS == gfpRepCountTU[3].RFS);
***/


Device[] InverterLibList=product(inverter, 200);
