num[] reverse_array(num[] arr)
{
    num size_arr = SIZE(arr);
    
    num[] rev;
    for(num i=0; i<size_arr; i=i+1) {    
    	rev[i] = arr[size_arr-i-1];
    }    
    return rev;
}

num[] arr = [1, 2, 3, 4];
num[] rev_arr = reverse_array(arr);

if(rev_arr != [4, 3, 2, 1]) {exit;}


