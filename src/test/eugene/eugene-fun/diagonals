num N = 9;
for(num i=N; i>0; i=i-1) {

    for(num j=1; j<i; j=j+1) {
        print(" ");
    }
    
    println(i);
}

for(num i=1; i<=10; i=i+1) {
    for(num j=1; j<=i; j=j+1) {
        print(" ");
    }
	println(i);
}
for(num i=9; i>=1; i=i-1) {
    for(num j=1; j<=i; j=j+1) {
        print(" ");
    }
	println(i);
}
