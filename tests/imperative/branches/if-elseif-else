/*---------------------------
 *   Variable vs Constant
 *---------------------------*/
num a = 5;
if ( a < 5 ) {
   println(" a < 5 ");
} elseif ( a != 5 ) {
   println(" a != 5 ");
} elseif ( a > 5 ) {
   println(" a > 5 ");
} else {
   println("THIS SHOULD HAPPEN");
}

/*---------------------------
 *   Variable vs Variable
 *---------------------------*/
num b = 6;
if ( a >= b ) {
   println(" a >= b ");
} elseif ( a > b ) {
   println(" a > b ");
} else {
   println("THIS SHOULD HAPPEN");
}

