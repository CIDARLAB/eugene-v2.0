/*
 * assignments of constants
 */
num i = 3;
println (i);

txt s = "Hello DNA!";
println (s);

txt[] sa = ["A", "T", "C", "G"];
println (sa);

num[] ia = [1, 2, 3.1415962, -1];
println (ia);

boolean b = true;
println (b);


/*
 *  assignments of function results
 */
PartType PT;
PT p1;
PT p2;
Device D(PT);
ds = product(D);
println(ds);

/*
 *  assignments when accessing sub-elements
 */
ia[0] = 0;
ia[1] = 0;
ia[2] = 0;
ia[3] = 0;
if(ia != [0.0, 0.0, 0.0, 0.0]) {exit;}

